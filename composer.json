{
    "name": "swoolecan/permission",
    "description": "hyperf权限管理组件",
    "keywords": [
        "swoolecan",
        "hyperf",
        "permission",
        "permissions",
        "roles",
        "acl",
        "rbac",
        "security"
    ],
    "homepage": "https://github.com/swoolecan/permission",
    "license": "MIT",
    "authors": [
        {
            "name": "swoolecan",
            "homepage": "https://github.com/donjan-deng",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-swoole": ">=4.5",
        "swoolecan/baseapp": "dev-master"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Swoolecan\\Permission\\": "src"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Swoolecan\\Permission\\Test\\": "tests"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Swoolecan\\Permission\\ConfigProvider"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
